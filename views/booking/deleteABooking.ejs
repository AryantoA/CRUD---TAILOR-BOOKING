<div class="container">
    <h1>Are you sure you want to delete this Booking ? </h1>
    <div class="col-lg-3">
        <form action="/consumers/booking/delete/<%= Booking.consumerId%>/<%= Booking.id%>" method="POST">
            <ul>
                <li>Booking ID :
                    <%= Booking.id%>
                </li>
                <li>Date of Booking :
                    <%= Booking.dateOfBooking%>
                </li>
                <li>Time of Booking :
                    <%= Booking.timeOfBooking%>
                </li>
                <li>Consumer ID :
                    <%= Booking.consumerId%>
                </li>
                <li>Tailor ID :
                    <%= Booking.tailorId%>
                </li>
            </ul>
            <input type="submit" value="Yes">
        </form>
        <button><a href="/consumers/booking/<%= Booking.consumerId%>/">No</a></button>

    </div>
</div>